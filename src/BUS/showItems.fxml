<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="showItems" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BUS.ShowItems">
   <children>
      <TableView fx:id="myItemsView" prefHeight="341.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="Item ID" />
          <TableColumn fx:id="titleCol" prefWidth="100.0" text="Titel" />
            <TableColumn fx:id="authCol" prefWidth="100.0" text="Author" />
            <TableColumn fx:id="avaCol" prefWidth="100.0" text="Beschikbaarheid" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox layoutY="341.0" prefHeight="70.0" prefWidth="600.0">
         <children>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#back" prefHeight="70.0" prefWidth="604.0" text="Ga terug" />
         </children>
      </HBox>
   </children>
</AnchorPane>
